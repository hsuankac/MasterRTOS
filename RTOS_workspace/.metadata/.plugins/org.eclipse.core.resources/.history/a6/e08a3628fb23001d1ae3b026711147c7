#include "main.h"

void menu_task(void* parameters)
{
	while(1)
	{

	}
}

void cmd_handler_task(void* parameters)
{
	BaseType_t ret;
	while(1)
	{
		ret = xTaskNotifyWait(0, 0, NULL, portMAX_DELAY);
		if(ret == pdTRUE)
		{
			process_cmd();
		}
	}
}

